package com.fykj.product.evaluation.api.scoreitem.model;

import java.math.BigDecimal;

import javax.persistence.Column;
import javax.persistence.Entity;
import javax.persistence.Table;

import com.fykj.platform.kernel._c.model.AbstractEntity;

/**
 * ClassName: ScorePoint  
 * (得分项)
 * @author zhangtian  
 * @version
 */
@Entity
@Table(name = "t_score_item")
public class ScoreItem extends AbstractEntity {

	private static final long serialVersionUID = 1L;
	@Column(name = "score_item_name")
	private String scoreItemName ;// 得分点名称
	@Column(name = "project_id")
	private String projectId ;// 项目ID
	@Column(name = "parent_quota")
	private String parentQuota ;// 上级指标
	@Column(name = "highest_score")
	private BigDecimal highestScore ;// 最高分
	@Column(name = "serial_number")
	private int serialNumber ;// 编号
	@Column(name = "data_collection_type")
	private String dataCollectionType ;// 数据采集形式
	@Column(name = "fill_person")
	private String fillPerson ;// 填报角色
	@Column(name = "preliminary_type")
	private String preliminaryType ;// 初评类型
	@Column(name = "self_person")
	private String selfPerson ;// 自评角色
	@Column(name = "preliminary_expect")
	private String preliminaryExpect ;// 初评专家
	@Column(name = "final_person")
	private String finalPerson ;// 终评角色
	@Column(name = "score_item_desc")
	private String scoreItemDesc ;// 得分项描述
	@Column(name = "remark")
	private String remark ;// 备注

	public String getScoreItemName() {
		return scoreItemName;
	}

	public void setScoreItemName(String scoreItemName) {
		this.scoreItemName = scoreItemName;
	}

	public String getParentQuota() {
		return parentQuota;
	}

	public void setParentQuota(String parentQuota) {
		this.parentQuota = parentQuota;
	}

	public BigDecimal getHighestScore() {
		return highestScore;
	}

	public void setHighestScore(BigDecimal highestScore) {
		this.highestScore = highestScore;
	}

	public int getSerialNumber() {
		return serialNumber;
	}

	public void setSerialNumber(int serialNumber) {
		this.serialNumber = serialNumber;
	}

	public String getDataCollectionType() {
		return dataCollectionType;
	}

	public void setDataCollectionType(String dataCollectionType) {
		this.dataCollectionType = dataCollectionType;
	}

	public String getFillPerson() {
		return fillPerson;
	}

	public void setFillPerson(String fillPerson) {
		this.fillPerson = fillPerson;
	}

	public String getPreliminaryType() {
		return preliminaryType;
	}

	public void setPreliminaryType(String preliminaryType) {
		this.preliminaryType = preliminaryType;
	}

	public String getSelfPerson() {
		return selfPerson;
	}

	public void setSelfPerson(String selfPerson) {
		this.selfPerson = selfPerson;
	}

	public String getPreliminaryExpect() {
		return preliminaryExpect;
	}

	public void setPreliminaryExpect(String preliminaryExpect) {
		this.preliminaryExpect = preliminaryExpect;
	}

	public String getFinalPerson() {
		return finalPerson;
	}

	public void setFinalPerson(String finalPerson) {
		this.finalPerson = finalPerson;
	}

	public String getRemark() {
		return remark;
	}

	public void setRemark(String remark) {
		this.remark = remark;
	}

	public String getScoreItemDesc() {
		return scoreItemDesc;
	}

	public void setScoreItemDesc(String scoreItemDesc) {
		this.scoreItemDesc = scoreItemDesc;
	}

	public String getProjectId() {
		return projectId;
	}

	public void setProjectId(String projectId) {
		this.projectId = projectId;
	}
}
